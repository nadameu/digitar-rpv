		"@babel/preset-typescript": "^7.3.2",
		"@types/intl": "^1.2.0",
		"@types/jest": "^24.0.18",
		"jest": "^24.9.0",
		"prettier": "^1.18.2",
		"rimraf": "^3.0.0",
		"rollup": "^1.20.2",
		"rollup-plugin-babel": "^4.3.3",
