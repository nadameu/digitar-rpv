{
	"name": "digitar-rpv",
	"version": "1.0.0",
	"description": "Digitar ofício requisitório",
	"main": "src/index.ts",
	"userscript": {
		"namespace": "http://nadameu.com.br/",
		"include": [
			"/^https:\\/\\/eproc\\.(jf(pr|rs|sc)|trf4)\\.jus\\.br\\/eproc(V2|2trf4)\\/controlador\\.php\\?acao=oficio_requisitorio_beneficiarioshonorarios_editar&/"
		],
		"grant": "none",
		"run-at": "document-start"
	},
	"scripts": {
		"build": "webpack --env.production --mode production",
		"build:dev": "webpack --env.development --mode development",
		"serve": "webpack-dev-server --env.development --mode development --open",
		"start": "npm run build:dev && npm run serve",
		"test": "jest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nadameu/digitar-rpv.git"
	},
	"author": "nadameu",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/nadameu/digitar-rpv/issues"
	},
	"browserlist": ["last 2 Chrome versions", "Firefox 38"],
	"homepage": "https://github.com/nadameu/digitar-rpv#readme",
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest",
			"^.+\\.jsx?$": "babel-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
	},
	"devDependencies": {
		"@types/intl": "^1.2.0",
		"@types/jest": "^22.2.0",
		"@types/node": "^9.4.7",
		"clean-webpack-plugin": "^0.1.19",
		"css-loader": "^0.28.11",
		"jest": "^22.4.3",
		"node-sass": "^4.7.2",
		"raw-loader": "^0.5.1",
		"sass-loader": "^6.0.7",
		"style-loader": "^0.20.3",
		"ts-jest": "^22.4.2",
		"ts-loader": "^4.1.0",
		"typescript": "^2.7.2",
		"userscript-meta": "^1.0.1",
		"webpack": "^4.2.0",
		"webpack-cli": "^2.0.12",
		"webpack-dev-server": "^3.1.1"
	},
	"dependencies": {
		"intl": "^1.2.5"
	}
}
